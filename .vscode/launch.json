{
	"version": "0.2.0",
	"name": "rajkovukovic.com",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Vite",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "integratedTerminal",
			"serverReadyAction": {
				"pattern": "localhost:(\\d+)",
				"uriFormat": "http://localhost:%s",
				"action": "openExternally"
			}
		}
	]
}
